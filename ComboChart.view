sum = function(arr) {
    return _.reduce(arr, (function(memo, num) {
        return memo + num;
    }), 0);
};

average = function(arr) {
    return _.reduce(arr, function(memo, num) {
        return memo + num;
    }, 0) / arr.length;
};

