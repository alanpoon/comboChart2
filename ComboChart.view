	aggreData = [];
	_.each(groupByArr, function(m, n) {
	nest.key(function(d) {
	return d[m];
	});
	});
	var result=[];
	nest.rollup(function(d) {
	var sampleObj=_.clone(d[0]);
	var sumValue=d3.sum(d, function(g) {return g.value; });
